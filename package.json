{
  "name": "the-lotr-api",
  "version": "1.0.0",
  "description": "app to get and format data about the Lord of The Rings movies",
  "main": "apps.js",
  "scripts": {
    "clean": "rm -rf .nyc_output build coverage && mkdir build",
    "build": "npm run clean && npm run build-babel",
    "start": "node ./build/app.js",
    "build-babel": "babel -d ./build ./src -s",
    "test": "cross-env NODE_ENV=test nyc mocha ./src/test",
    "lint": "eslint \"**/*.js\"",
    "dev": "nodemon --exec babel-node src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supercede/the-lotr-api.git"
  },
  "keywords": [
    "lotr",
    "movies"
  ],
  "author": "Supercede",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/supercede/the-lotr-api/issues"
  },
  "homepage": "https://github.com/supercede/the-lotr-api#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-paginate": "^1.0.0",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "redis": "^2.8.0"
  },
  "engines": {
    "node": "12.x"
  }
}
